msgid ""
"\n"
"Thanks,\n"
"The %(project_name)s team\n"
msgstr ""
"\n"
"Obrigado,\n"
"Equipe %(project_name)s\n"

#: templates/_emails/change_email-body-html.html:3
#: templates/_emails/change_email-subject.html:1
